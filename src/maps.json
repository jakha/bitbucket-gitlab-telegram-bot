{
    "gitlab_mr_v13": {
        "user": "user.name",
        "project": "project.name",
        "description": "object_attributes.description",
        "metainfo": "project.description",
        "url": "object_attributes.url",
        "sourse": "object_attributes.source_branch",
        "target": "object_attributes.target_branch"
    },
    "bitbucket_pr_v" : {
	"user": "actor.displayName",
        "project": "pullRequest.fromRef.repository.project.name",
        "description": "pullRequest.description",
        "metainfo": "pullRequest.fromRef.repository.project.description",
        "url": "pullRequest.links.self[0].href",
        "sourse": "pullRequest.fromRef.displayId",
        "target": "pullRequest.toRef.displayId"
    }
}
